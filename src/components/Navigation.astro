---
import BackAndForwardIcons from "./Back_and_forward_icons.astro";
import NavigationsButtonstyleLinks from "./Navigations_buttonstyle_links.astro";
const hasSearch = Astro.props; 
---

<article>
    <BackAndForwardIcons/>
    {hasSearch && <input type="text" />}
    <NavigationsButtonstyleLinks/>
</article>

<style>
article{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    background-color: rgb(15, 15, 15);
    padding: 5px;
}
</style>